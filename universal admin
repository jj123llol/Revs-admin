if getgenv().executed == true then warn("\nadmin is already running\nadmin is already running\nadmin is already running\nadmin is already running") return end

getgenv().executed = true

local admin = {
fc = {}
}

local CommandEvents = {
["AntiFling"] = {
["Events"] = {
["PlayerAdded"] = nil,
["CharacterAdded"] = {},
["ChildAdded"] = {},
["Constraints"] = {}
}
},
}

admin.fc.MakeCollisionConstraints = function(FlingPart)
CommandEvents.AntiFling.Events.Constraints[FlingPart] = {}

for _, Object in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
if Object:IsA("BasePart") then
local NoCollisionConstraint = Instance.new("NoCollisionConstraint")

NoCollisionConstraint.Part0 = Object
NoCollisionConstraint.Part1 = FlingPart
            
table.insert(CommandEvents.AntiFling.Events.Constraints[FlingPart], NoCollisionConstraint)

NoCollisionConstraint.Enabled = true
NoCollisionConstraint.Parent = Object
end
end
end

admin.fc.AddFling = function(Part)
if Part:IsA("BasePart") and not CommandEvents.AntiFling.Events.Constraints[Part] then
admin.fc.MakeCollisionConstraints(Part)
end
end

if game:GetService("Players").LocalPlayer.PlayerScripts:FindFirstChild("ChatScript",true) then
local ChatMain = require(game:GetService("Players").LocalPlayer.PlayerScripts.ChatScript.ChatMain)
local function bypass()
ChatMain.MessagePosted:fire("dffhdfshfdfdfdfdfdf"..math.random(9e9,9e9))
ChatMain.MessagesChanged:fire(math.random(9e9,9e9))
end
end

function chat(msg)
if game:GetService("Players").LocalPlayer.PlayerScripts:FindFirstChild("ChatScript",true) then
game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(msg,"All")
else
game.TextChatService.ChatInputBarConfiguration.TargetTextChannel:SendAsync(msg)
end
end

function resetcmd()
orgin = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
task.spawn(function()
game.Players.LocalPlayer.Character.Humanoid.Health = 0; wait(0.1); game.Players.LocalPlayer.Character:Destroy() ;game.Players.LocalPlayer.Character = nil; repeat wait() until game.Players.LocalPlayer.Character ~= nil
game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = orgin
end)
end

