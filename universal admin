if cmd == "antifling" then
CommandEvents.AntiFling.Events.PlayerAdded = game.Players.PlayerAdded:Connect(function(Player)
table.insert(CommandEvents.AntiFling.Events.CharacterAdded, Player.CharacterAdded:Connect(function(Character)
table.insert(CommandEvents.AntiFling.Events.ChildAdded, Character.ChildAdded:Connect(admin.fc.AddFling))
for _, Object in pairs(Player.Character:GetDescendants()) do
admin.fc.AddFling(Object)
end
end))
for _, Object in pairs(Player.Character:GetDescendants()) do
admin.fc.AddFling(Object)
end
end)
for _, Player in pairs(game.Players:GetPlayers()) do
if Player ~= game.Players.LocalPlayer then
table.insert(CommandEvents.AntiFling.Events.CharacterAdded, Player.CharacterAdded:Connect(function(Character)
table.insert(CommandEvents.AntiFling.Events.ChildAdded, Player.Character.ChildAdded:Connect(admin.fc.AddFling))
for _, Object in pairs(Player.Character:GetDescendants()) do
admin.fc.AddFling(Object)
 end
end))
for _, Object in pairs(Player.Character:GetDescendants()) do
admin.fc.AddFling(Object)
end
end
end
end

if cmd == "unantifling" then
if CommandEvents.AntiFling.Events.PlayerAdded then
CommandEvents.AntiFling.Events.PlayerAdded:Disconnect()
end
for _, Event in pairs(CommandEvents.AntiFling.Events.CharacterAdded) do
Event:Disconnect()
end
for _, Event in pairs(CommandEvents.AntiFling.Events.ChildAdded) do
Event:Disconnect()
end
for _, FPart in pairs(CommandEvents.AntiFling.Events.Constraints) do
for _, Constraint in pairs(FPart) do
Constraint:Destroy()
end
end
CommandEvents.AntiFling.Events.Constraints = {}
end

