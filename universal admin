                    local function system(msg)
                        local text = "<font color ='#da8541'>[SCRIPT]</font>: " .. msg .. ""
                        local TextMessage = Instance.new("TextLabel", DisplayMessages)
                        local TextMessagePadding = Instance.new("UIPadding", TextMessage)
