function getPlayer(name)
    local targets = {}
    local plrstbl = game.Players:GetPlayers()
    if name == "me" then
        return {game.Players.LocalPlayer}
    elseif name == "others" then
        for i, v in pairs(plrstbl) do
            if v ~= game.Players.LocalPlayer then
                targets[#targets + 1] = v
            end
        end
    elseif name == "all" then
        for i, v in pairs(plrstbl) do
            targets[#targets + 1] = v
        end
    elseif name == "random" then
        for i, v in pairs(plrstbl) do
            if v ~= game.Players.LocalPlayer then
                return {plrstbl[math.random(1, #plrstbl)]}
            end
        end
    else
        name = name:lower()
        for i, v in pairs(game.Players:GetPlayers()) do
            if v.Name:lower():find(name) or v.DisplayName:lower():find(name) then
                return {v}
            end
        end
    end
    return targets
end

function veryepictrol(x)
    local prefix = getgenv().selectedprefix
    local d, d1
    d =
        x.Chatted:Connect(
        function(msg)
            msg = msg:lower()
            if string.sub(msg, 1, 3) == "/e " then
                msg = string.sub(msg, 4)
            end
            if string.sub(msg, 1, 1) == prefix then
                local cmd
                local space = string.find(msg, " ")
                if space then
                    cmd = string.sub(msg, 2, space - 1)
                else
                    cmd = string.sub(msg, 2)
                end

                for _, v in pairs(plugins) do
                    if cmd == v then
                        if space ~= nil then
                            Who = string.sub(msg, space + 1)
                            target = getPlayer(Who)[1]
                        end

                        if isfile("Revs admin/Plugins/" .. v .. "") then
                            loadstring(readfile("Revs admin/Plugins/" .. v .. ""))()
                        elseif isfile("Revs admin/Plugins/" .. v .. ".txt") then
                            loadstring(readfile("Revs admin/Plugins/" .. v .. ".txt"))()
                        else
                            loadstring(readfile("Revs admin/Plugins/" .. v .. ".lua"))()
                        end
                    end
                end

                if cmd == "msglogger" then
                    if chatlogger then
                        chatlogger:Disconnect()
                    end
                    if game.Players.LocalPlayer:WaitForChild("PlayerGui"):FindFirstChild("ChatLogger") then
                        game.Players.LocalPlayer:FindFirstChild("PlayerGui"):FindFirstChild("ChatLogger"):Destroy()
                    end
                    local sg = Instance.new("ScreenGui", game.Players.LocalPlayer:WaitForChild("PlayerGui"))
                    local ChatGui = Instance.new("Frame", sg)
                    local Close = Instance.new("ImageButton", ChatGui)
                    local CloseCorner = Instance.new("UICorner", Close)
                    local SexyGuiMainCorner = Instance.new("UICorner", ChatGui)
                    local DisplayMessages = Instance.new("ScrollingFrame", ChatGui)
                    local DisplayMessageLayout = Instance.new("UIListLayout", DisplayMessages)
