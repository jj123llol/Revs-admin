char:WaitForChild("HumanoidRootPart").Velocity = Vector3.new(5e3,5e3,5e3)
char:WaitForChild("HumanoidRootPart").CFrame = ((ftarget.Character:WaitForChild("HumanoidRootPart").CFrame + Vector3.new(0,0.1,0) + ftarget.Character:WaitForChild("Humanoid").MoveDirection * 0.5) * CFrame.Angles(0,30,0))
end)
end


