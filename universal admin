notify("Fling","Stopping...",5)
repeat wait(); char:WaitForChild("HumanoidRootPart").Velocity = Vector3.new(-5,-5,-5); char:WaitForChild("HumanoidRootPart").CFrame = orgcframe until (tick() - stopped > 5)
tflingrunning2 = false
end
end
end)
end

