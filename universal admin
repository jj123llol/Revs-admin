function unequip()
for _,v in pairs(char:GetChildren()) do
if v:IsA("Tool") then
v.Parent = game.Players.LocalPlayer.Backpack
end
end
end

function waitforstop()
task.spawn(function()
while tflingrunning == true do
wait()
repeat wait(); until ftarget.Character:WaitForChild("HumanoidRootPart").Velocity.X > 90 or ftarget.Character:WaitForChild("HumanoidRootPart").Velocity.Z > 90 or ftarget == nil
if tfling then tfling:Disconnect()
char = game.Players.LocalPlayer.Character
stopped = tick()
notify("TFling","Stopping...",3)
repeat wait(); char:WaitForChild("HumanoidRootPart").Velocity = Vector3.new(0,0,0); char:WaitForChild("HumanoidRootPart").CFrame = orgcframe until (tick() - stopped > 2)
unequip()
tflingrunning = false
end
end
end)
end

